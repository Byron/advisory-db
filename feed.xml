      &lt;dd&gt;&lt;a href=&quot;https://nvd.nist.gov/vuln-metrics/cvss/v3-calculator?vector=CVSS:3.1&amp;#x2f;AV:L&amp;#x2f;AC:L&amp;#x2f;PR:L&amp;#x2f;UI:N&amp;#x2f;S:C&amp;#x2f;C:N&amp;#x2f;I:L&amp;#x2f;A:H&quot;&gt;CVSS:3.1&amp;#x2f;AV:L&amp;#x2f;AC:L&amp;#x2f;PR:L&amp;#x2f;UI:N&amp;#x2f;S:C&amp;#x2f;C:N&amp;#x2f;I:L&amp;#x2f;A:H&lt;/a&gt;&lt;/dd&gt;
      

      &lt;dt id=&quot;patched&quot;&gt;Patched&lt;/dt&gt;
