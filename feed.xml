      &lt;/dd&gt;
      

      

      

      
      &lt;dt id=&quot;details&quot;&gt;Details&lt;/dt&gt;
      &lt;dd&gt;
        &lt;a href=&quot;https://github.com/3Hren/msgpack-rust/issues/305&quot;&gt;
