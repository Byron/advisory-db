        &lt;ul&gt;
          
          &lt;li&gt;&lt;code&gt;&amp;lt;0.10.48, &amp;gt;=0.9.7&lt;/code&gt;&lt;/li&gt;
          
        &lt;/ul&gt;
      &lt;/dd&gt;
      
      &lt;dt&gt;&lt;code&gt;openssl::x509::X509Extension::new_nid&lt;/code&gt;&lt;/dt&gt;
      &lt;dd&gt;
        &lt;ul&gt;
          
          &lt;li&gt;&lt;code&gt;&amp;lt;0.10.48, &amp;gt;=0.9.7&lt;/code&gt;&lt;/li&gt;
          
