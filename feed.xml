    &lt;header&gt;
      &lt;h1&gt;
        
        RUSTSEC-2020-0166
        
      &lt;/h1&gt;
      &lt;span class=&quot;subtitle&quot;&gt;&lt;p&gt;personnummer Input validation error&lt;/p&gt;
&lt;/span&gt;
    &lt;/header&gt;

    

    &lt;dl&gt;
      &lt;dt id=&quot;reported&quot;&gt;Reported&lt;/dt&gt;
      &lt;dd&gt;
        &lt;time datetime=&quot;2020-09-04&quot;&gt;
          September  4, 2020
        &lt;/time&gt;
