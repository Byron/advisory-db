      &lt;dd&gt;
        
        
        &lt;a href=&quot;/packages/mail-internals.html&quot;&gt;mail-internals&lt;/a&gt;
          (&lt;a href=&quot;https://crates.io/crates/mail-internals&quot;&gt;crates.io&lt;/a&gt;)
        
        
